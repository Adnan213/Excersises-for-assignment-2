void setup() {
  size (400,400);
  background( 90 );
  float [] y = new float [50] ;
  float [] x = new float [50];
  //loop fucntion
  
  for(int i=0; i<50; i++){
    y[i]=int(random(400));
    x[i]=int(random(400));
      fill( 0);
      stroke(0);
      point(x[i] , y[i]);
      
  if( y[i]>200){
    line(x[i], y[i], x[i], 400);
  }else{
    line(x[i], y[i], x[i], 0);
    
  }
  }
  //
  //generate values <50

  
  
}
